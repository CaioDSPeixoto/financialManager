{
  "name": "financial",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint": "yarn check-types && next lint",
    "test": "jest --watch",
    "test:coverage": "jest--watch --coverage",
    "start": "env-cmd -f .env.local next dev",
    "develop": "env-cmd -f .env.development next dev",
    "staging": "env-cmd -f .env.staging next start",
    "production": "env-cmd -f .env.production next start",
    "build:start": "env-cmd -f .env.local next build && next export -o build/",
    "build:development": "env-cmd -f .env.development next build && next export -o build/",
    "build:staging": "env-cmd -f .env.staging next build && next export -o build/",
    "build:production": "env-cmd -f .env.production next build && next export -o build/"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.4",
    "@supabase/auth-helpers-nextjs": "^0.7.2",
    "@supabase/auth-helpers-react": "^0.4.0",
    "@supabase/auth-ui-react": "^0.4.2",
    "@supabase/supabase-js": "^2.24.0",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "env-cmd": "^10.1.0",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "next": "13.4.4",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.9.0",
    "react-select": "^5.7.3",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3"
  }
}
